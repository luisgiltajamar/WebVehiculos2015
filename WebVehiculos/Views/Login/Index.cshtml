@model WebVehiculos.Models.Usuario

@{
    ViewBag.Title = "Index";
}

@using (Html.BeginForm())
{
    @Html.LabelFor(o=>o.Login)
    @Html.TextAreaFor(o=>o.Login)
    @Html.ValidationMessageFor(o=>o.Login)
    <br/>
    @Html.LabelFor(o => o.Password)
    @Html.TextAreaFor(o => o.Password)
    @Html.ValidationMessageFor(o => o.Password)
    <br />
    <input type="submit" value="Enviar"/>
}
